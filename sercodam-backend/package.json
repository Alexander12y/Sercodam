{
  "name": "sercodam-backend",
  "version": "1.0.0",
  "description": "Backend API para sistema de órdenes de producción SERCODAM",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "build": "npm run migrate && npm run seed",
    "setup-webhook": "node setup-webhook-config.js",
    "test-webhook": "node test-pdf-and-webhook.js",
    "test-webhook-only": "node test-webhook-only.js",
    "test-pdf": "node -e \"require('./test-pdf-and-webhook.js').testPDFGeneration().catch(console.error)\"",
    "test-make": "node -e \"require('./test-pdf-and-webhook.js').testMakeWebhook().catch(console.error)\"",
    "test-orden-completa": "node test-orden-completa.js",
    "test-pdf-automatico": "node test-pdf-webhook-automatico.js",
    "test-pdf-download": "node test-pdf-download.js",
    "test-pdf-api": "node test-pdf-api-download.js",
    "cleanup-pdfs": "node cleanup-pdfs.js",
    "reset-migrations": "node reset-migrations.js"
  },
  "keywords": [
    "express",
    "postgresql",
    "redis",
    "jwt",
    "api"
  ],
  "author": "SERCODAM",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
